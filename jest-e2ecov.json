{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "modulePathIgnorePatterns": [
    "<rootDir>/dist",
    "<rootDir>/.*/__mocks__",
    "<rootDir>/.*/__tests__"
  ],
  "collectCoverageFrom": [
    "**/*.{!(mock|util|error|entity|dto|enum),}.ts"
  ],
  "coverageDirectory": "./coverage/e2e",
  "moduleNameMapper": {
    "^\\#utils/(.*)$": "<rootDir>/utils/$1",
    "^\\#test/(.*)$": "<rootDir>/test/$1",
    "^\\#app/(.*)$": "<rootDir>/src/$1",
    "^\\#users/(.*)$": "<rootDir>/src/users/$1",
    "^\\#members/(.*)$": "<rootDir>/src/members/$1",
    "^\\#channels/(.*)$": "<rootDir>/src/channels/$1"
  }
}